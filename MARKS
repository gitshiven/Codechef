#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,n,x,y;
	cin>>t;
	while(t--){
	    cin>>n>>x>>y;
	    int c=0;
	    for(int i=0;i<=n;i++)
	    {
	        if(i*x==y)
	        c++;
	    }
	    if(c>0)
	      cout<<"yes"<<endl;
	     else
	       cout<<"no"<<endl;
	}
	return 0;
}
