//STARTERS 65

#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,k;
	    cin>>n>>k;
	    int a[n];
	    int flag=0;
	    for(int i=0; i<n;i++){
	        cin>>a[i];
	        if(a[i]==k && i!=n-1){
	            flag =1;
	        }
	    }
	    if(n==1 && k==a[0]){
	        cout<<"Yes\n";
	        continue;
	    }
	    if(!flag){
	        cout<<"No"<<endl;
	    }
	    else{
	        cout<<"yes"<<endl;
	    }
	}
	return 0;
}

