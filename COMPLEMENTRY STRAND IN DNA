#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n,i;
	    cin>>n;
	    string s;
	    cin>>s;
	    for(i=0;i<n;i++){
	        if(s[i]=='A')
	            s[i]='T';
	        else if(s[i]=='T')
	            s[i]='A';
	        else if(s[i]=='C')
	            s[i]='G';
	        else if(s[i]=='G')
	            s[i]='C';
	    }
	    cout<<s<<endl;
	}
	return 0;
}

//Using char
#include <iostream>
using namespace std;

int main() {
	short T,N;   //short datatype used for small integers.
	cin>>T;
	while(T--)
	{
	    cin>>N;
	    char a[N];
	    cin>>a;
	    for(short i=0;i<N;i++)
	    {
	        if(a[i]=='A')
	        a[i]='T';
	        else if(a[i]=='T')
	        a[i]='A';
	        else if(a[i]=='G')
	        a[i]='C';
	        else
	        a[i]='G';
	    }
	    cout<<a<<endl;
	}
	return 0;
}
