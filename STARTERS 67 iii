//Counting Problem

#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int a[n];
	    int cnt=0;
	    for(int i=0; i<n; i++){
	        cin>>a[i];
	        if(a[i]%2)
	        cnt++;
	    }
	    if(cnt==0 || cnt%2)
	    cout<<"NO"<<endl;
	    else
	    cout<<"YES"<<endl;
	}
	return 0;
}
